<odoo>
    <data>
        <record id="report_deadline_urgent_view_tree" model="ir.ui.view">
            <field name="name">report.deadline.urgent.view.tree</field>
            <field name="model">report.deadline.urgent</field>
            <field name="arch" type="xml">
                <tree string="Deadline Urgent">
                    <field name="task_code" string="Task Code"/>
                    <field name="task_name" string="Task Name"/>
                    <field name="project_id" string="Project"/>
                    <field name="member" string="Member"/>
                    <field name="deadline" string="Deadline"/>
                    <field name="status" string="Status"/>
                </tree>
            </field>
        </record>

        <record id="report_deadline_urgent_action" model="ir.actions.act_window">
            <field name="name">Report Deadline Urgent</field>
            <field name="res_model">report.deadline.urgent</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>