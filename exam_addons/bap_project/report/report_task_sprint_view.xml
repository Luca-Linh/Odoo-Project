<odoo>
    <record id="view_report_task_sprint_tree" model="ir.ui.view">
        <field name="name">view.report.task.sprint.tree</field>
        <field name="model">report.task.sprint</field>
        <field name="arch" type="xml">
            <tree>
                <field name="member_id"/>
                <field name="project_id"/>
                <field name="sprint_id"/>
                <field name="role"/>
                <field name="total_task_count"/>
                <button name="action_view_total_tasks" string="All task" type="object" class="btn-primary"/>
                <field name="task_new_count"/>
                <button name="action_view_new_tasks" string="New task" type="object" class="btn-primary"/>
                <field name="task_dev_count"/>
                <button name="action_view_dev_tasks" string="Dev task" type="object" class="btn-primary"/>
                <field name="task_qc_count"/>
                <button name="action_view_qc_tasks" string="Qc task" type="object" class="btn-primary"/>
                <field name="task_done_count"/>
                <button name="action_view_done_tasks" string="Done task" type="object" class="btn-primary"/>
                <field name="sprint_start_date"/>
                <field name="sprint_end_date"/>
            </tree>
        </field>
    </record>
</odoo>
