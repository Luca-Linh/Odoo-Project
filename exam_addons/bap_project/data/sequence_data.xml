<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="project_sequence_id" model="ir.sequence">
      <field name="name">Project Sequence</field>
      <field name="code">project_sequence_code</field>
      <field name="prefix">PRJ</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
    </record>

    <record id="sprint_sequence_id" model="ir.sequence">
      <field name="name">Sprint Sequence</field>
      <field name="code">sprint_sequence_code</field>
      <field name="prefix">SPR</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
    </record>

    <record id="task_sequence_id" model="ir.sequence">
      <field name="name">Task Sequence</field>
      <field name="code">task_sequence_code</field>
      <field name="prefix">T</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
    </record>

    <record id="task_type_sequence_id" model="ir.sequence">
      <field name="name">Task Type Sequence</field>
      <field name="code">task_type_sequence_code</field>
      <field name="prefix">TT</field>
      <field name="padding">3</field>
      <field name="number_next">1</field>
    </record>

    <record id="open_sequence_id" model="ir.sequence">
      <field name="name">Open Sequence</field>
      <field name="code">open_sequence_code</field>
      <field name="prefix">ROP</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
    </record>

    <record id="close_sequence_id" model="ir.sequence">
      <field name="name">Close Sequence</field>
      <field name="code">close_sequence_code</field>
      <field name="prefix">RCP</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
    </record>

  </data>
</odoo>